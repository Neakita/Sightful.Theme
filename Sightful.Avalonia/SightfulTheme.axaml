<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Styles.Resources>
		<ResourceDictionary>
			<ResourceDictionary.ThemeDictionaries>
                <ResourceDictionary x:Key="Dark">
                    <ResourceDictionary.MergedDictionaries>
                        <MergeResourceInclude Source="ThemeVariants/DarkThemeColors.axaml"/>
                        <MergeResourceInclude Source="SpatialTheme.axaml"/>
                        <MergeResourceInclude Source="AppearanceTheme.axaml"/>
                    </ResourceDictionary.MergedDictionaries>
                </ResourceDictionary>
			</ResourceDictionary.ThemeDictionaries>
		</ResourceDictionary>
	</Styles.Resources>
	<StyleInclude Source="DefaultControls.axaml"/>
	<StyleInclude Source="Styles/UserControl.axaml"/>
	<StyleInclude Source="Styles/Border.axaml"/>
	<StyleInclude Source="Controls/Buttons/Styles/RegularButtonContent.axaml"/>
    <StyleInclude Source="Controls/Buttons/Styles/CardButtonContent.axaml"/>
	<StyleInclude Source="Styles/Padding.axaml"/>
	<StyleInclude Source="Styles/ListBoxItem.axaml"/>
	<StyleInclude Source="Styles/StackPanel.axaml"/>
	<StyleInclude Source="Styles/Control.axaml"/>
    <StyleInclude Source="Styles/MaterialIcon.axaml"/>
    <StyleInclude Source="Styles/Grid.axaml"/>
    <StyleInclude Source="MaterialIconText.axaml"/>
</Styles>