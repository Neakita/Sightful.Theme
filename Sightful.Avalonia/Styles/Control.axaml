	<Style Selector=":is(Control)">
		<Setter Property="ToolTip.Placement" Value="{DynamicResource ToolTipPlacement}"/>
		<Setter Property="ToolTip.VerticalOffset" Value="{DynamicResource ToolTipVerticalOffset}"/>
	</Style>
</Styles>
